{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "doxyfile",
      "camelcase": "Doxyfile",
      "title": "Doxyfile",
      "scope": "source.doxyfile",
      "file-types": [
        "Doxyfile"
      ],
      "injection-regex": "^[Dd]oxy(file|gen)$",
      "class-name": "TreeSitterDoxyfile",
      "highlights": "queries/highlights.scm",
      "injections": "queries/injections.scm",
      "tags": "queries/tags.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A tree sitter grammar for Doxyfiles",
    "authors": [
      {
        "name": "tinger",
        "email": "tinger@tinger.dev"
      }
    ],
    "links": {
      "repository": "https://github.com/tingerrr/tree-sitter-doxyfile"
    }
  },
  "bindings": {
    "c": false,
    "go": false,
    "node": true,
    "python": false,
    "rust": false,
    "swift": false,
    "zig": false
  }
}
