{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "doxyfile",
  "rules": {
    "doxyfile": {
      "type": "REPEAT",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "comment"
              },
              {
                "type": "SYMBOL",
                "name": "option"
              }
            ]
          },
          {
            "type": "STRING",
            "value": "\n"
          }
        ]
      }
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "SYMBOL",
            "name": "comment_body"
          }
        }
      ]
    },
    "comment_body": {
      "type": "PATTERN",
      "value": "[^\\n]*"
    },
    "option": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "+="
            },
            {
              "type": "STRING",
              "value": "="
            }
          ]
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "value"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        }
      ]
    },
    "value": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_ENL"
          },
          {
            "type": "SYMBOL",
            "name": "literal"
          }
        ]
      }
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[_a-zA-Z][_a-zA-Z0-9]*"
    },
    "literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "boolean"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "unquoted_string"
        }
      ]
    },
    "boolean": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "YES"
        },
        {
          "type": "STRING",
          "value": "NO"
        }
      ]
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "quoted_string": {
      "type": "PATTERN",
      "value": "\"[^\"]*\""
    },
    "unquoted_string": {
      "type": "PATTERN",
      "value": "[^\" \\t\\n\\\\]+"
    },
    "_ENL": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\\"
        },
        {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "STRING",
            "value": "\n"
          }
        }
      ]
    }
  },
  "extras": [
    {
      "type": "STRING",
      "value": " "
    },
    {
      "type": "STRING",
      "value": "\t"
    },
    {
      "type": "STRING",
      "value": "\n"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}